    "@supabase/supabase-js": "^2.39.0",
    "@types/jszip": "^3.4.0",
    "axios": "^1.6.0",
    "expo": "^53.0.0",
    "expo-apple-authentication": "~7.2.4",
    "expo-asset": "~11.1.7",
    "expo-build-properties": "0.14.8",
