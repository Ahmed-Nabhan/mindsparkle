{
  "name": "mindsparkle",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "dev:client": "npx expo start",
    "eas:build:ios": "npx eas build --profile development --platform ios --wait",
