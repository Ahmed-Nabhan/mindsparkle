{
  "outputDirectory": "docs",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/landing",
      "destination": "/landing/index.html"
    },
    {
      "source": "/landing/(.*)",
      "destination": "/landing/index.html"
    },
    {
      "source": "/privacy",
      "destination": "/privacy.html"
    },
    {
      "source": "/terms",
      "destination": "/terms.html"
    },
    {
      "source": "/support",
      "destination": "/support.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
