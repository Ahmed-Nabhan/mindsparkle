{
  "expo": {
    "name": "MindSparkle",
    "slug": "mindsparkle",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1E3A8A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mindsparkle.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1E3A8A"
      },
      "package": "com.mindsparkle.app"
    },
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ]
  }
}
